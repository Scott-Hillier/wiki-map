<div class="point-submission-box">
  <div style="display: flex; justify-content: space-between">
    <h3 class="point-form-dynamic-title"></h3>
    <p class="close-point-form" style="cursor: pointer">close</p>
  </div>
  <form class="edit-point">
    <div class="form-field required">
      <label for="point-title">Title:</label>
      <input type="text" id="point-title" placeholder="e.g. Lighthouse Labs!" />
    </div>

    <div class="form-field required">
      <label for="point-description">Description:</label>
      <input
        type="text"
        id="point-description"
        placeholder="e.g. A place to learn"
      />
    </div>

    <div class="form-field required">
      <label for="point-image-url">Image thumbnail URL: </label>
      <input
        type="text"
        id="point-image-url"
        placeholder="e.g. data:image/jpeg;base64,"
      />
    </div>

    <div class="form-field">
      <label for="point-address">Address: </label>
      <input
        type="text"
        id="point-address"
        placeholder="e.g. 401 West Georgia #600, Vancouver, BC"
      />
    </div>
    <div class="form-field">
      <label for="point-type">Type: </label>
      <input
        type="text"
        id="point-type"
        placeholder="e.g. academic institution"
      />
    </div>
    <input type="text" id="point-latitude" hidden />
    <input type="text" id="point-longitude" hidden />
    <input type="text" id="point-mapId" hidden />
    <input type="text" id="point-pointId" hidden />
    <input type="text" id="point-update" hidden />

    <button class="point-form-dynamic-button"></button>
  </form>
</div>
